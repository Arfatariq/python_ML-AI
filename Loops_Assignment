#print numbers froom 1 to 10
for i in range(1,11):
    print(i)
# multipplication table of 5
num=5
for num in range(1,11):
    result= num*i
    print("f{num}*{i} = {result}")
        
#sqr of each no from 1 to 5
for  i in range(1,6):
    print(i**2)
#in reverse order
for i in range(1,11,-1):
    print(i)


#print each character of a string
string = 'hello'     
for char in string:
    print(char)     
#print elemenst of a list
my_list=[10,20,30,40,40]
for i in my_list:
    print(i)
#ikntermediate level=---------------------
num=5
for i in range(1,11):
    result=num*i
    print(num,'*',i,'=', result)
#find the sum of all number from 1 to 100
sum=0
for i in range(1,101):
    sum = sum+i
    print(sum)
#fibonacci seriees
num= 10
a= 0
b= 1
count=0
while count<num:
    print(a)
    c=a+b
    a=b
    b=c
    count=count+1
#count the number of bowels
string = 'This is a python program to print vowels'
vowels = 0

for char in string:
    if char.lower() in 'aeiou':
        vowels = vowels + 1

print("Number of vowels:", vowels)
#number of consents

string='THIS Is a phython program to print consents'
consents=0
for char in string:
    if char.lower() in 'bcdfghjklmnpqrstvxyz':
        consents+=1
print('conents are :',consents)
#print upper and lower case in a string
string='This Is A Phython Program to print LOWER and upper cases'
upper=0
lower=0
for char in string:
    if char.isupper():
        upper+=1
    elif char.islower():
        lower+=1
print('uppercase letters',upper)
print('lower letters',lower)
text = "banana"
letter = 'a'
count = 0
#count a in text
for char in text:
    if char == letter:
        count += 1

print(f"'{letter}' appears {count} times")


        
